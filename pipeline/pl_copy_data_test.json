{
	"name": "pl_copy_data_test",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"additionalColumns": [
							{
								"name": "id",
								"value": "$$COLUMN:$.value[0].MemberMlsId"
							}
						],
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"additionalHeaders": {
							"Authorization": "Bearer e1d82e9443044b9b89a57a685f952218"
						},
						"paginationRules": {
							"AbsoluteUrl": "$.{@odata.nextLink}"
						}
					},
					"sink": {
						"type": "CosmosDbSqlApiSink",
						"writeBehavior": "upsert",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"enableSkipIncompatibleRow": true,
					"logSettings": {
						"enableCopyActivityLog": true,
						"copyActivityLogSettings": {
							"logLevel": "Warning",
							"enableReliableLogging": false
						},
						"logLocationSettings": {
							"linkedServiceName": {
								"referenceName": "rappatoni_data_lake",
								"type": "LinkedServiceReference"
							},
							"path": "rappatoni-df/logs/"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_cincy_rest",
						"type": "DatasetReference",
						"parameters": {
							"UrlPath": "/Member?Class=Member&$filter=MemberMlsId ne ''"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ds_cosmosdb",
						"type": "DatasetReference",
						"parameters": {
							"DbContainer": "Member"
						}
					}
				]
			}
		],
		"variables": {
			"KV_Secret_Names": {
				"type": "Array",
				"defaultValue": [
					"client-id",
					"client-secret"
				]
			},
			"TrueVar": {
				"type": "String"
			},
			"FalseVar": {
				"type": "String"
			},
			"ExpirationVar": {
				"type": "String"
			}
		},
		"folder": {
			"name": "Test"
		},
		"annotations": []
	}
}